<% unless cart.line_items.empty? %>
<div class="cart_title">seu carrinho de compras</div>
<table>
	<%= render(cart.line_items)%>
<tr class="total_line">
	<td colspan="2">Total</td>
	<td class="total_cell"><%= number_to_currency(cart.total_price) %></td> </tr>
</table>
<%= button_to "Finalizar Compra", new_order_path, method: :get %>
<%= button_to "Esvaziar Carrinho", cart, method: :delete, confirm: "Deletar Carrinho?" %>
<% end %>